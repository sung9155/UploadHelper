<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="WindowForegroundBrush" Color="#000000"/>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="ButtonForegroundBrush" Color="#000000"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#CCCCCC"/>
    <SolidColorBrush x:Key="ButtonHoverBrush" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="ButtonPressedBrush" Color="#D0D0D0"/>
    <SolidColorBrush x:Key="ListBoxBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ListBoxForegroundBrush" Color="#000000"/>
    <SolidColorBrush x:Key="ListBoxBorderBrush" Color="#CCCCCC"/>
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FFCCCCCC"/>
</ResourceDictionary> 