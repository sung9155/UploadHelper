<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#1E1E1E"/>
    <SolidColorBrush x:Key="WindowForegroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#3E3E3E"/>
    <SolidColorBrush x:Key="ButtonForegroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#555555"/>
    <SolidColorBrush x:Key="ListBoxBackgroundBrush" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="ListBoxForegroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ListBoxBorderBrush" Color="#555555"/>
    <SolidColorBrush x:Key="TextBlockForegroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ComboBoxBackgroundBrush" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ComboBoxForegroundBrush" Color="#000000"/>
    <SolidColorBrush x:Key="ComboBoxBorderBrush" Color="#555555"/>
</ResourceDictionary> 